
<div class="span9 content">
  <%= create_guide("Create Asset", 
        "Office: <b>#{current_office.name}</b>" + "<br />"    +  
        "Total Clients: <b>#{current_office.clients.count}</b>"     ).html_safe %>
  
  <%#= create_breadcrumb(@breadcrumbs).html_safe %>
 
  <div class="row-fluid">
    <div id="data_list">
      
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Client</th>
            <th>Total Asset</th>
          </tr>
        </thead>
        <tbody>
          <% current_office.clients.each do |client| %>
            <tr>
              <td><%= link_to client.name, new_client_asset_url(client) %></td>
              <td><%= client.assets.count  %></td>
            </tr>
          <% end %>

        </tbody>
      </table>
    </div><!-- end of #data_list -->
  </div>

</div><!--/span-->















